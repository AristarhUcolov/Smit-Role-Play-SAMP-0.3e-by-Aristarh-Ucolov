				RB[playerid] = block;
				block++;
				new ftext[5];
				switch(PlayerInfo[playerid][pMRInections])
				{
				case 1: ftext = "LSPD";
				case 10: ftext = "SFPD";
				case 21: ftext = "LVPD";
				}
				rbtime[playerid] = 600;
				format(stringer,sizeof(stringer),"Сотрудник %s %s установил дорожное загрождение.",ftext,PlayerInfo[playerid][pNames],RB[playerid]);
				SendTeamMessage(1, 0x4169E1FF, stringer);
				SetPlayerPosAC(playerid,X,Y,Z+1);
			}
			else
			{
				DestroyObject(BlockPost[RB[playerid]]);
				DestroyDynamic3DTextLabel(Block3D[RB[playerid]]);
				RB[playerid] = 255;
				rbtime[playerid] = 0;
			}
		}
		return true;
