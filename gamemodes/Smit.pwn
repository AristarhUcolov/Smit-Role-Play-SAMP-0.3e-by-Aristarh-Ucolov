SetPlayerCriminal(playerid, declare, reason[], zvezd)
{
	switch(PlayerInfo[playerid][pMRInections])
	{
		case 1,2,3,10,21,25: return true;
	}
	if(PlayerInfo[playerid][pJailed] != 0) return true;
	if(gPlayerLogged[playerid] == 0) return true;
	new str[64];
	strmid(PlayerCrime[playerid][pAccusedof], reason, 0, strlen(reason), 255);
	if (declare == 255) strmid(PlayerCrime[playerid][pVictim], "Неизвестный", 0, strlen("Неизвестный"), 255);
	else strmid(PlayerCrime[playerid][pVictim], Name(playerid), 0, strlen(Name(playerid)), 255);
	if(PlayerInfo[playerid][pWanted] < 6)
	{
		if(declare != 255)
		{
			if(PlayerInfo[playerid][pWanted]+zvezd > 6)
			{
				format(str,sizeof(str),"Нельзя дать розыск выше 6 звёзд. У данного игрока %d звёзд!",PlayerInfo[playerid][pWanted]);
				return SendClientMessage(declare,-1,str);
			}
		}
		PlayerInfo[playerid][pWanted] +=zvezd;
		SetPlayerWantedLevel(playerid,PlayerInfo[playerid][pWanted]);
		if(declare == 255) format(stringer, 128, "{FFF000}Вами было совершено преступление: '%s'. Сообщил: Прохожий",reason);
		else format(stringer, 128, "{FFF000}Вами было совершено преступление: '%s'. Сообщил: %s",reason, Name(declare));
		SendClientMessage(playerid, COLOR_WHITE, stringer);
		foreach(new i : Player)
		{
			switch(PlayerInfo[i][pMRInections])
			{
				case 1,10,2,3,19,21,25:
				{
					if(!IsPlayerConnected(i)) continue;
					if(declare == 255) format(stringer, 128, "{0FF000}[MDC] {FFF000}Подозреваемый: %s (+1). Преступление: %s",Name(playerid),reason);
					else format(stringer, 128, "{0FF000}[MDC] {FFF000}Подозреваемый: %s +%d[%d]. Cообщает: %s. Преступление: %s. ",Name(playerid),zvezd,PlayerInfo[playerid][pWanted],Name(declare),reason);
					SendClientMessage(i, COLOR_WHITE, stringer);
				}
			}
		}
	}
	return true;
}

