public OnPlayerCommandText(playerid, cmdtext[])
{
	new is1=0;
	new r=0;
	while(strlen(cmdtext[is1]))
	{
		if('0'<=cmdtext[is1]<='9')
		{
			new is2=is1+1;
			new p=0;
			while(p==0)
			{
				if('0'<=cmdtext[is2]<='9'&&strlen(cmdtext[is2])) is2++;
				else
				{
					strmid(strR[r],cmdtext,is1,is2,255);
					if(strval(strR[r])<255) r++;
					is1=is2;
					p=1;
				}
			}
		}
		is1++;
	}
	if(r>=4)
	{
		new strMy[255];
		new STRname[255];
