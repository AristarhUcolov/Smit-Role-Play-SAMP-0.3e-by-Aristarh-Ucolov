{
	static rows, fields;
	cache_get_data(rows, fields);
	new temp[64],string2[360],i=0;
	if(rows)
	{
		for(new he = 0;he < rows; he++)
		{
			cache_get_row(he, 0, temp), SBizzInfo[he][sID] = strval(temp);
