		{
			if(RB[playerid] == 255)
			{
				new Float:X,Float:Y,Float:Z,Float:A;
				GetPlayerPos(playerid,X,Y,Z);
				GetPlayerFacingAngle(playerid, A);
				BlockPost[block] = CreateObject(981,X,Y,Z-0.2,0.00,0.00,A+180);
				format(stringer,sizeof(stringer),"Блок пост №%d\n{FF0000}Установил %s.",block,PlayerInfo[playerid][pNames]);
