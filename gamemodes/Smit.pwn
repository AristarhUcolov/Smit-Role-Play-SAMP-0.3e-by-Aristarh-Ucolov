stock AddAdmin(targetid, cena, lvl)
{
    if(PlayerInfo[targetid][pAdmInections]) return 1;
    if(PlayerInfo[targetid][pDMInections] < cena) return SCMF(targetid, 0xFF0000AA, "[Ошибка] {ffffff}Для приобретения Административных прав данного уровня Вам нужно %d рублей",cena);
    PlayerInfo[targetid][pAdmInections] = lvl;
    PlayerInfo[targetid][pDMInections] -= cena;
    if(PlayerInfo[targetid][pDostup] == 0 || PlayerInfo[targetid][pDostup] == 2038) PlayerInfo[targetid][pDostup] = 124+random(8999);
    SCMF(targetid, -1, "Поздравляем, вы купили Административные права, ваш пароль от Административной панели: {0ff000}%d", PlayerInfo[targetid][pDostup]);
