publics: DeathmatchEnded()
{
    if(DeathmatchKill[0] > DeathmatchKill[1])
    {
	    foreach(new i : Player)
        {
            if(IsPlayerConnected(i))
	        {
                if(PlayerDeathmatch[i] == 1)
                {
                    PlayerDeathmatch2[i] = 1;
                    SendClientMessage(i, 0xBFFF00AA, "Ваша команда победила! Поздравляем!");
		            SendMes(i,0xBFFF00AA,"На ваш донат счет зачислено 10 доната.");
		            PlayerInfo[i][pCash] += 10000;
                }
            }
        }
    }
    else if(DeathmatchKill[0] < DeathmatchKill[1])
    {
	    foreach(new i : Player)
        {
            if(IsPlayerConnected(i))
	        {
                if(PlayerDeathmatch[i] == 2)
                {
                    PlayerDeathmatch2[i] = 1;
                    SendClientMessage(i, 0xBFFF00AA, "Ваша команда победила! Поздравляем!");
                    SendMes(i,0xBFFF00AA,"На ваш донат счет зачислено 10 доната.");
		            PlayerInfo[i][pCash] += 10000;
                }
            }
        }
    }
    else if(DeathmatchKill[0] == DeathmatchKill[1])
    {
        foreach(new i : Player)
        {
            if(IsPlayerConnected(i))
	        {
                if(PlayerDeathmatch[i] != 0)
                {
			        SendClientMessage(i, 0xBFFF00AA, "Ничья между командами");
			        PlayerDeathmatch2[i] = 0;
                }
            }
        }
    }
    DeathmatchPlayers = 0;
	DeathmatchKill[0] = 0, DeathmatchKill[1] = 0;
	DeathmatchAmout[0] = 0,	DeathmatchAmout[1] = 0;
	DeathmatchONRound = 0, LeftTime = 0;
	SendClientMessageToAll(0xBFFF00AA,"[КБ] Командный бой окончен. (( Список победителей: /teamlist ))");
    foreach(new i : Player)
	{
	    if(IsPlayerConnected(i))
	    {
	        if(PlayerDeathmatch[i] != 0)
	        {
	            Delgun(i);
	            PlayerDeathmatch[i] = 0;
	           	SetPlayerPosAC(i, -2168.1282,643.4412 + random(2)+1,1052.3750);
	           	SetPlayerFacingAngle(i, 188.8227);
	           	SetPlayerVirtualWorld(i, 21);
	           	SetPlayerInterior(i, 1);
	           	PlayerInfo[i][pInt] = 1;
	           	SetPlayerArmourAC(i, 0);
	            SetPlayerHealthAC(i, 100);
				if(PlayerInfo[i][pMRInections] > 0) SetPlayerSkin(i, PlayerInfo[i][pModel]);
                else if(PlayerInfo[i][pMRInections] <= 0) SetPlayerSkin(i, PlayerInfo[i][pChar]);
                SetPlayerToTeamColor(i);
		        SetCameraBehindPlayer(i);
	            CP[i] = 0;
	            DisablePlayerRaceCheckpoint(i);
	            TextDrawHideForPlayer(i,Textdraw0), TextDrawHideForPlayer(i,Textdraw1);
	        	TextDrawHideForPlayer(i,Textdraw2), TextDrawHideForPlayer(i,Textdraw3);
		        TextDrawHideForPlayer(i,Textdraw4), TextDrawHideForPlayer(i,Textdraw5);
		        TextDrawHideForPlayer(i,Textdraw6), TextDrawHideForPlayer(i,Textdraw7);
		        for(new w = 0; w < MAX_PLAYER_ATTACHED_OBJECTS; w++)
                {
                    if(IsPlayerAttachedObjectSlotUsed(i, w)) RemovePlayerAttachedObject(i, w);
                }
	        }
		}
	}
	return true;
}
