{
	if(IsPlayerInAnyVehicle(playerid))
	{
		new vehicleid; vehicleid = GetPlayerVehicleID(playerid);
		new locked[32],str[64],str1[64],str2[64],str4[64],str5[64],Float:vehhel,vehh[20],vehs[20],vehe[20];
