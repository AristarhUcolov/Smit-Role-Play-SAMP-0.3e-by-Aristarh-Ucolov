forward SAMP_AC_SEND_WARNING_FOR_MODE(CALLERID, const SAMP_AC_NAME[], SAC_CODE, SAMP_AC_DOOM);
public SAMP_AC_SEND_WARNING_FOR_MODE(CALLERID, const SAMP_AC_NAME[], SAC_CODE, SAMP_AC_DOOM)
{
    if(SAC_CODE == 1) return 1;
    if(TeleportTime[CALLERID] > 0) return 1;
    if(PlayerInfo[CALLERID][pAdmInections]) return true;
    new string[128];
    if(SAMP_AC_DOOM == 1)
    {
        if(GetPVarInt(CALLERID, "time_warning") < gettime())
        {
            format(string, sizeof(string),"Игрок %s[%d] возможно использует Чит. %s {ffffff}Ошибка: (#%02d)",PlayerInfo[CALLERID][pNames],CALLERID,SAMP_AC_NAME,SAC_CODE);
            SendAdminMessage(COLOR_LIGHTRED, string);
            SetPVarInt(CALLERID, "time_warning", gettime() + 5);
        }
    }
    else
    {
    	if(GetPVarInt(CALLERID, "time_warning_kick") < gettime())
        {
	        format(string, sizeof(string), "{FF0000}[Античит]{ff0000}Вы были кикнуты по подозрении в читерстве. {ffffff}Ошибка: (#%02d)", SAC_CODE);
	        SendClientMessage(CALLERID, 0x6BB3FFAA, string);
	        format(string, sizeof(string), "Игрок %s[%d] кикнут за использование читов: %s {ffffff}Ошибка: (#%02d)", PlayerInfo[CALLERID][pNames],CALLERID, SAMP_AC_NAME, SAC_CODE);
	        SendAdminMessage(COLOR_LIGHTRED, string);
	        SetPVarInt(CALLERID, "time_warning_kick", gettime() + 5);
  		}
        Kick(CALLERID);
    }
    return true;
}
public OnPlayerSpawn(playerid)
{
	SetTimerEx("UnFreez",1500, 0, "d", playerid);
	PlayerInfo[playerid][text_gren_zone] = 1;
	SetPlayerScore(playerid, PlayerInfo[playerid][pLevel]);
	StopAudioStreamForPlayer(playerid);
	SetPVarInt(playerid,"K_Times",0);
	TeleportTime[playerid] = 4;
	AntiFakeKill[playerid] = true;
	SetPVarInt(playerid, "NoAB", 1);
	TextDrawHideForPlayer(playerid, greenzone);
	if(PlayerInfo[playerid][pMRInections] > 0) SetPlayerSkin(playerid, PlayerInfo[playerid][pModel]);
	if(PlayerInfo[playerid][pMRInections] <= 0) SetPlayerSkin(playerid, PlayerInfo[playerid][pChar]);
	AntiDM[playerid] = 0; TogglePlayerControllable(playerid, 1);
	SetPlayerSpawn(playerid);
	SetPlayerHealthAC(playerid, 100);
 	if(!PlayerToPoint(40.0,playerid,5508.3706,1244.7594,23.1886) && PlayerInfo[playerid][pJailedDeMorgantime] > 0)//Если время в форте еще не закончилось, то
    {
