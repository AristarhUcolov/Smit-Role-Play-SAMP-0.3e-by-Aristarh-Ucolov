stock GetGangZoneColor(gangzonex)
{
	new zx;
	switch(GZInfo[gangzonex][gFrakVlad])
	{
	case 12: zx = 0xd200ff99;
	case 18: zx = 0x007fffAA;
	case 13: zx = 0xcfbe0899;
	case 15: zx = 0x06b50699;
	case 17: zx = 0x03c3b899;
	default: zx = 0xFFBE0065;
	}
	return zx;
}
