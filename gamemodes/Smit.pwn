		else vehs = "";
		if(zavodis[playerid] == 1) vehe = "~g~ON";
		else vehe = "~r~OFF";
		format(str, sizeof(str),"%03i",SpeedVehicle(playerid));
		format(str1, sizeof(str1),"HEAL:%s%.0f",vehh,vehhel / 10);
		format(str2, sizeof(str2), "%s", locked);
		format(str4, sizeof(str4), "FUEL:%s%.0f", vehs,Fuell[GetPlayerVehicleID(playerid)]);
		format(str5, sizeof(str5), "%s", vehe);
