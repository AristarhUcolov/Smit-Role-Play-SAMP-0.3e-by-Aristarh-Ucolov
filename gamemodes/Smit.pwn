}
stock IsPlayerInBandOnline(bandid)
{
	foreach(Player,i)
	{
		if(!IsPlayerConnected(i) || gPlayerLogged[i] == 0) continue;
		if(PlayerInfo[i][pMRInections] == bandid || PlayerInfo[i][pLRInections] == bandid) return true;
	}
