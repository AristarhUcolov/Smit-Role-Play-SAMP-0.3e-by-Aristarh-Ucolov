        SetPlayerVirtualWorld(playerid,1);
		SetPlayerPosAC(playerid, 5508.3706,1244.7594,23.1886);//Сажаем обратно в форт
        SendClientMessage(playerid,COLOR_GREY,"За повторную попытку сбежать из деморгана вы можете получить кик.");
    }
	SetTimerEx("UnfreezePlayer", 1000, false, "i",playerid);
	return true;
}
