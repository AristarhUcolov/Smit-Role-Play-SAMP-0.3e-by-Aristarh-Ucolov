stock GzCheck()
{
    new namegang[24],namegangv[24];
 	//1 капт в час
	if(capturez1 == true)
    {
		if(Capturetime[0] > 0) Capturetime[0]--;
		foreach(new id : Player)
        {
			if(PlayerInfo[id][pMRInections] == Command[1] || PlayerInfo[id][pMRInections] == Command[0])
			{
				switch(Command[1])
				{
				case 15: namegang = "Grove";
				case 17: namegang = "Aztecas";
				case 12: namegang = "Ballas";
				case 18: namegang = "Rifa";
				case 13: namegang = "Vagos";
				}
				switch(Command[0])
				{
				case 15: namegangv = "Grove";
				case 17: namegangv = "Aztecas";
				case 12: namegangv = "Ballas";
				case 18: namegangv = "Rifa";
				case 13: namegangv = "Vagos";
				}
                format(stringer,sizeof(stringer), "%s ~g~~h~%d",namegang,ScoreZahvat[Command[1]]);
				TextDrawSetString(GangWar1[5],stringer);
				format(stringer,sizeof(stringer), "%s ~g~~h~%d",namegangv,ScoreZahvat[Command[0]]);
				TextDrawSetString(GangWar1[4],stringer);
				format(stringer,sizeof(stringer),"Time:~r~~h~%s",convert(Capturetime[0]));
				TextDrawSetString(GangWar1[2],stringer);
                GangZoneFlashForAll(GZInfo[WarZoneID1][gID],COLOR_REDD);
			}
		}
		if(Capturetime[0] == 0)
		{
			if(ScoreZahvat[Command[0]] > ScoreZahvat[Command[1]])
			{
				SendFamilyMessage(Command[0],COLOR_WHITE,"{00FF00}[Внимание] Вам удалось отстоять свою територию.");
				SendFamilyMessage(Command[1],COLOR_WHITE,"{FF0000}[Внимание] Вам не удалось захватить новую територию.");
                GangZoneStopFlashForAll(WarZoneID1);
				foreach(Player, p) { if(PlayerInfo[p][pMRInections] == Command[0] || PlayerInfo[p][pMRInections] == Command[1]) TextDrawHideForPlayer(p,GangWar1[0]),TextDrawHideForPlayer(p,GangWar1[1]),TextDrawHideForPlayer(p,GangWar1[2]),TextDrawHideForPlayer(p,GangWar1[3]),TextDrawHideForPlayer(p,GangWar1[4]),TextDrawHideForPlayer(p,GangWar1[5]); }
				ScoreZahvat[Command[0]] = 0;
				ScoreZahvat[Command[1]] = 0;
				Command[0] = 0;
				Command[1] = 0;
			}
			else
			{
				SendFamilyMessage(Command[0],COLOR_WHITE,"{FF0000}[Внимание] Вы потеряли контроль над одной из своих територий.");
				SendFamilyMessage(Command[1],COLOR_WHITE,"{00FF00}[Внимание] Вам удалось захватить новую територию.");
                GangZoneStopFlashForAll(WarZoneID1);
				GangZoneHideForAll(WarZoneID1);
				switch(Command[1])
				{
				case 12: GangZoneShowForAll(GZInfo[WarZoneID1][gID],0xd200ff99);
				case 18: GangZoneShowForAll(GZInfo[WarZoneID1][gID],0x007fffAA);
				case 13: GangZoneShowForAll(GZInfo[WarZoneID1][gID],0xcfbe0899);
				case 15: GangZoneShowForAll(GZInfo[WarZoneID1][gID],0x06b50699);
				case 17: GangZoneShowForAll(GZInfo[WarZoneID1][gID],0x03c3b899);
				default: GangZoneShowForAll(GZInfo[WarZoneID1][gID],0x00000000);
				}
				foreach(Player, p) { if(PlayerInfo[p][pMRInections] == Command[0] || PlayerInfo[p][pMRInections] == Command[1]) TextDrawHideForPlayer(p,GangWar1[0]),TextDrawHideForPlayer(p,GangWar1[1]),TextDrawHideForPlayer(p,GangWar1[2]),TextDrawHideForPlayer(p,GangWar1[3]),TextDrawHideForPlayer(p,GangWar1[4]),TextDrawHideForPlayer(p,GangWar1[5]); }
				ScoreZahvat[Command[0]] = 0;
				ScoreZahvat[Command[1]] = 0;
				GZInfo[WarZoneID1][gFrakVlad] = Command[1];
				Command[0] = 0;
				Command[1] = 0;
			}
			Capturetime[0] = 0;
			SaveGZ();
			capturez1 = false;
		}
	}
	//2 капт в час
	if(capturez2 == true)
    {
		if(Capturetime[1] > 0) Capturetime[1]--;
		foreach(new id : Player)
        {
			if(PlayerInfo[id][pMRInections] == Command[3] || PlayerInfo[id][pMRInections] == Command[2])
			{
				switch(Command[3])
				{
				case 15: namegang = "Grove";
