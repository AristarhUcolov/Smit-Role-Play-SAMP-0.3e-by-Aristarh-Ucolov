			if(GetPlayerVehicleID(playerid) == caridhouse[playerid])
			{
				PlayerInfo[playerid][pFuelcar] -= 0.003;
			}
		}
		new carid = GetPlayerVehicleID(playerid);
		if(SuperGt(carid))
		{
			if(SpeedVehicle(playerid) !=0)
			{
				Fuell[vehicleid] -= 0.005;
				if(GetPlayerVehicleID(playerid) == caridhouse[playerid])
				{
					PlayerInfo[playerid][pFuelcar] -=0.005;
				}
			}
		}
		if(Fuell[carid] < 0)
		{
			Fuell[carid] = 0;
			GetVehicleParamsEx(GetPlayerVehicleID(playerid),engine,lights,alarm,doors,bonnet,boot,objective);
			SetVehicleParamsEx(GetPlayerVehicleID(playerid),VEHICLE_PARAMS_OFF,VEHICLE_PARAMS_OFF,alarm,doors,bonnet,boot,objective); zavodis[playerid] = 0;
		}
		PlayerTextDrawSetString(playerid,SpeedometrDraws[10][playerid],str);
		PlayerTextDrawSetString(playerid,SpeedometrDraws[13][playerid],str1);
		PlayerTextDrawSetString(playerid,SpeedometrDraws[14][playerid],str2);
		PlayerTextDrawSetString(playerid,SpeedometrDraws[12][playerid],str4);
		PlayerTextDrawSetString(playerid,SpeedometrDraws[15][playerid],str5);
		new temptext[256];
		new speed = SpeedVehicle(playerid);
		if(speed>0)
    	{
		    format(temptext, sizeof(temptext), "");
			for(new dspeed; dspeed<floatround((16.0/100.3)*float(speed)) && dspeed<19; dspeed++)
			{
			    switch(dspeed)
			    {
			        case 7: format(temptext, sizeof(temptext), "%s~y~", temptext);
			        case 13: format(temptext, sizeof(temptext), "%s~r~", temptext);
			    }
			    format(temptext, sizeof(temptext), "%sI", temptext);
			}
			PlayerTextDrawSetString(playerid, SpeedometrDraws[3][playerid], temptext);
