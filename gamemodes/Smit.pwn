				GangZoneFlashForAll(GZInfo[WarZoneID2][gID],COLOR_REDD);
			}
		}
		if(Capturetime[1] == 0)
		{
			if(ScoreZahvat[Command[2]] > ScoreZahvat[Command[3]])
			{
				SendFamilyMessage(Command[2],COLOR_WHITE,"{00FF00}[Внимание] Вам удалось отстоять свою територию.");
				SendFamilyMessage(Command[3],COLOR_WHITE,"{FF0000}[Внимание] Вам не удалось захватить новую територию.");
                GangZoneStopFlashForAll(WarZoneID2);
